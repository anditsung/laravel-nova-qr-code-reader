{
    "name": "tsungsoft/qr-code-reader",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "type": "library",
    "authors": [
        {
            "name": "AndiTsung",
            "email": "anditsung@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Tsungsoft\\QrCodeReader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tsungsoft\\QrCodeReader\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
